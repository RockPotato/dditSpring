<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="rangerVo" class="kr.or.ddit.ranger.model.RangerVO">
		<property name="userId" value="brown"></property>
		
		<property name="birth" value="08-08-2018"/>
		<property name="reg_dt" value="2019-02-27"/>
	</bean>
	
	<!-- FormattingConversionServiceFactoryBean의 이름은 conversionService로 정의 되어 있다 -->
	<bean id="conversionService" 
			class="org.springframework.format.support.FormattingConversionServiceFactoryBean"/>
	
</beans>
